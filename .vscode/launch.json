{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "dezog",
            "request": "launch",
            "name": "Debug OpenMSX",
            "remoteType": "openmsx",
            "listFiles": [
                {
                    "path": "machines/msx/hello.lst",
                    "useFiles": true,
                    "asm": "sjasmplus",
                    "mainFile": "machines/msx/helloworld.asm"
                }
            ],
            "openmsx": {
                //"pcInSlot": "1 X 14" // nextor driver (rookie mapper)
                //"pcInSlot": "3 2 28" // usbether in 512kb mapper
            },
            "commandsAfterLaunch": [
                "-e openmsx_info version"
            ],
            "resetOnLaunch": false,
            "rootFolder": "${workspaceFolder}/machines/msx",
            "tmpDir": ".tmp"
        },
        {
			"type": "dezog",
			"request": "launch",
			"name": "Dezog-Agon-Hello",
			"rootFolder": "${workspaceFolder}/machines/msx",
			"remoteType": "agonelectronhal",
			"agonelectronhal": {
				"serial":"/dev/tty.usbserial-21130",
				"baudrate": 500000
			},
			"sjasmplus": [
				{
					"path": "general/bios.sld",
				},
				{
					"path": "helloworld/hello.sld",
				}
			],
			"startAutomatically": true,
			"loadObjs": [
				{ "path": "general/bios.rom", "start": "0x0000.5" },
				{ "path": "helloworld/hello.rom", "start": "0x4000.5" }
			],
			"topOfStack": "0xf380",
			"commandsAfterLaunch": [
				//"-exec e load -0 msx/bios.rom",
				//"-exec e load -1 msx/bios.rom",
				//"-exec e load -0 msx/kvalley.rom",
                // "-exec e load msx/hello.rom",
				"-exec e run"
            ],
			"preLaunchTask": "build"
		},
		,
        {
			"type": "dezog",
			"request": "launch",
			"name": "Dezog-Agon-snsmat",
			"rootFolder": "${workspaceFolder}/machines/msx",
			"remoteType": "agonelectronhal",
			"agonelectronhal": {
				"serial":"/dev/tty.usbserial-21130",
				"baudrate": 500000
			},
			"sjasmplus": [
				{
					"path": "general/bios.sld",
				},
				{
					"path": "snsmat/snsmat.sld",
				}
			],
			"startAutomatically": true,
			"loadObjs": [
				{ "path": "general/bios.rom", "start": "0x0000.5" },
				{ "path": "snsmat/snsmat.rom", "start": "0x4000.5" }
			],
			"topOfStack": "0xf380",
			"commandsAfterLaunch": [
				"-exec e init",
				"-exec e run"
            ],
			"preLaunchTask": "build"
		},
        {
			"type": "dezog",
			"request": "launch",
			"name": "Dezog-Agon-wrtpsg",
			"rootFolder": "${workspaceFolder}/machines/msx",
			"remoteType": "agonelectronhal",
			"agonelectronhal": {
				"serial":"/dev/tty.usbserial-21130",
				"baudrate": 500000
			},
			"sjasmplus": [
				{
					"path": "general/bios.sld",
				},
				{
					"path": "wrtpsg/wrtpsg.sld",
				}
			],
			"startAutomatically": true,
			"loadObjs": [
				{ "path": "general/bios.rom", "start": "0x0000.5" },
				{ "path": "wrtpsg/wrtpsg.rom", "start": "0x4000.5" }
			],
			"topOfStack": "0xf380",
			"commandsAfterLaunch": [
				"-exec e vsync on",
				"-exec e run"
            ],
			"preLaunchTask": "build"
		},
        {
			"type": "dezog",
			"request": "launch",
			"name": "Dezog-Agon-screen2-test",
			"rootFolder": "${workspaceFolder}/machines/msx",
			"remoteType": "agonelectronhal",
			"agonelectronhal": {
				"serial":"/dev/tty.usbserial-21130",
				"baudrate": 500000
			},
			"sjasmplus": [
				{
					"path": "general/bios.sld",
				},
				{
					"path": "screen2-test/test.sld",
				}
			],
			"startAutomatically": true,
			"loadObjs": [
				{ "path": "general/bios.rom", "start": "0x0000.5" },
				{ "path": "screen2-test/test.rom", "start": "0x4000.5" }
			],
			"topOfStack": "0xf380",
			"commandsAfterLaunch": [
				"-exec e init",
				"-exec e vsync on",
				"-exec e run"
            ],
			"preLaunchTask": "build"
		},
        {
			"type": "dezog",
			"request": "launch",
			"name": "Dezog-MOS-BBC",
			"rootFolder": "${workspaceFolder}/machines/msx",
			"remoteType": "agonelectronhal",
			"agonelectronhal": {
				"serial":"/dev/tty.usbserial-21130",
				"baudrate": 500000
			},
			"sjasmplus": [
				{
					"path": "bios.sld",
				}
			],
			"startAutomatically": false,
			"loadObjs": [
			],
			"topOfStack": "0xf380",
			"commandsAfterLaunch": [
                "-exec e load bbc/bbcbasic.bin"
            ],
			"preLaunchTask": "build"
		},
        {
			"type": "dezog",
			"request": "launch",
			"name": "Dezog-Agon-kingsvalley",
			"rootFolder": "${workspaceFolder}/machines/msx",
			"remoteType": "agonelectronhal",
			"agonelectronhal": {
				"serial":"/dev/tty.usbserial-21130",
				"baudrate": 500000
			},
			"sjasmplus": [
				{
					"path": "general/bios.sld",
				}
			],
			"startAutomatically": true,
			"loadObjs": [
				{ "path": "general/bios.rom", "start": "0x0000.5" },
				{ "path": "kingsvalley/kvalley.rom", "start": "0x4000.5" }
			],
			"topOfStack": "0xf380",
			"commandsAfterLaunch": [
				"-exec e init",
				"-exec e vsync on",
				//"-exec b 54086",
				//"-exec b 5402a",
				"-exec e run"
            ],
			"preLaunchTask": "build"
		}
    ]
}