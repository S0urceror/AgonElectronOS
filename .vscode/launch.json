        {
            "type": "dezog",
            "request": "launch",
            "name": "Debug OpenMSX",
            "remoteType": "openmsx",
            "listFiles": [
                {
                    "path": "machines/msx/hello.lst",
                    "useFiles": true,
                    "asm": "sjasmplus",
                    "mainFile": "machines/msx/helloworld.asm"
                }
